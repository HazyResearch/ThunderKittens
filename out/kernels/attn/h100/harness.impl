    bwd_prep_globals bwd_g{prep_og_arg, prep_o_arg, prep_d_arg};

    // backward pass
    /*
    DPCT1026:623: The call to cudaFuncSetAttribute was removed because SYCL
    currently does not support corresponding setting.
    */
    std::cout << "Set max dynamic memory!" << std::endl;

    dpct::dim3 grid_bwd(ATTN_N / (4 * kittens::TILE_ROW_DIM<bf16> * 4),
                        QO_HEADS, ATTN_B);
    static_assert(ATTN_N % (4*kittens::TILE_ROW_DIM<bf16>*4) == 0);
    dpct::get_current_device().queues_wait_and_throw();
    std::cout << "Starting bwd prep warmup" << std::endl;
    for(int i = 0; i < ITER; i++) {
        /*
        DPCT1129:441: The type "bwd_prep_globals<128>" is used in the SYCL
        kernel, but it is not device copyable. The sycl::is_device_copyable
        specialization has been added for this type. Please review the code.
        */
        auto exp_props = sycl::ext::oneapi::experimental::properties{
            sycl::ext::oneapi::experimental::use_root_sync};
        dpct::has_capability_or_fail(dpct::get_in_order_queue().get_device(),
                                     {sycl::aspect::fp16});

        dpct::get_in_order_queue().submit([&](sycl::handler &cgh) {
            sycl::local_accessor<uint8_t, 1> dpct_local_acc_ct1(
                sycl::range<1>(mem_size), cgh);

            cgh.depends_on(
                dpct::get_current_device().get_in_order_queues_last_events());

            cgh.parallel_for(
                sycl::nd_range<3>(grid_bwd * sycl::range<3>(1, 1, (32 * 4)),
                                  sycl::range<3>(1, 1, (32 * 4))),
                exp_props,
                [=](sycl::nd_item<3> item_ct1)
                    [[sycl::reqd_sub_group_size(32)]] {
                        bwd_attend_prep_ker<ATTN_D>(
                            bwd_g,
                            dpct_local_acc_ct1
                                .get_multi_ptr<sycl::access::decorated::no>()
                                .get());
                    });
        });
    }
    dpct::get_current_device().queues_wait_and_throw();
    std::cout << "Starting bwd prep kernel" << std::endl;
    const auto start_bwd = std::chrono::high_resolution_clock::now();
    for(int i = 0; i < ITER; i++) {
        /*
        DPCT1129:442: The type "bwd_prep_globals<128>" is used in the SYCL
        kernel, but it is not device copyable. The sycl::is_device_copyable
        specialization has been added for this type. Please review the code.
        */
        auto exp_props = sycl::ext::oneapi::experimental::properties{
            sycl::ext::oneapi::experimental::use_root_sync};
        dpct::has_capability_or_fail(dpct::get_in_order_queue().get_device(),
                                     {sycl::aspect::fp16});

        dpct::get_in_order_queue().submit([&](sycl::handler &cgh) {
            sycl::local_accessor<uint8_t, 1> dpct_local_acc_ct1(
                sycl::range<1>(mem_size), cgh);

            cgh.depends_on(
                dpct::get_current_device().get_in_order_queues_last_events());

            cgh.parallel_for(
                sycl::nd_range<3>(grid_bwd * sycl::range<3>(1, 1, (32 * 4)),
                                  sycl::range<3>(1, 1, (32 * 4))),
                exp_props,
                [=](sycl::nd_item<3> item_ct1)
                    [[sycl::reqd_sub_group_size(32)]] {
                        bwd_attend_prep_ker<ATTN_D>(
                            bwd_g,
                            dpct_local_acc_ct1
                                .get_multi_ptr<sycl::access::decorated::no>()
                                .get());
                    });
        });
    }
    dpct::get_current_device().queues_wait_and_throw();
    const auto finish_bwd = std::chrono::high_resolution_clock::now();
    CudaCheckError();
    std::cout << "Finished bwd prep kernel\n";

    dpct::get_in_order_queue()
