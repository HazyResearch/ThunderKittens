    d_v = sycl::malloc_device<bf16>((TOTAL_ELEMENTS_KV),
                                    dpct::get_in_order_queue());
    d_o = sycl::malloc_device<bf16>((TOTAL_ELEMENTS_QO),
                                    dpct::get_in_order_queue());

    d_l = sycl::malloc_device<float>((TOTAL_ELEMENTS_QO / ATTN_D),
