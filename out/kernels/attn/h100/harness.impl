    std::ofstream diff_file("printouts/o_diff.txt");

    float total_diff = 0;
    for(int i = 0; i < TOTAL_ELEMENTS_QO; i++) {
        float diff = o[i] - o_ref[i % (TOTAL_ELEMENTS_QO/ATTN_B)];
